        "silex/silex"           : "~1.0.0",
        "symfony/config"        : "~2.3",
        "symfony/console"       : "~2.3",
        "symfony/form"          : "~2.3",
        "symfony/finder"        : "~2.2",
        "symfony/translation"   : "~2.3",
        "symfony/twig-bridge"   : "~2.3",
        "symfony/validator"     : "~2.3",
        "symfony/yaml"          : "~2.3",
